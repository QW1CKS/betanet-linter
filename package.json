    "fs-extra": "^11.1.1",
    "temp-write": "^4.0.0",
    "execa": "^5.1.1",
  "which": "^3.0.1",
  "cbor": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@types/js-yaml": "^4.0.5",
    "@types/xml2js": "^0.4.11",
    "@types/fs-extra": "^11.0.1",
    "@types/which": "^3.0.0",
    "typescript": "^5.1.6",
    "ts-node": "^10.9.1",
  "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
