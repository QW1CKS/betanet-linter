{
  "name": "betanet-compliance-linter",
  "version": "1.0.0",
  "description": "CLI tool for checking Betanet specification compliance",
  "main": "dist/index.js",
  "bin": {
    "betanet-lint": "bin/cli.js"
  },
  "scripts": {
